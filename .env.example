# MARBA Supabase Configuration
# Get these from https://app.supabase.com/project/_/settings/api
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
VITE_SUPABASE_SERVICE_ROLE_KEY=your_service_role_key
SUPABASE_DB_PASSWORD=your_database_password

# Brandock Supabase (for NAICS migration only)
# Only needed if migrating industry profiles from Brandock
BRANDOCK_SUPABASE_URL=your_brandock_url
BRANDOCK_SUPABASE_KEY=your_brandock_key

# OpenRouter API (for AI content generation)
VITE_OPENROUTER_API_KEY=your_openrouter_api_key

# Optional: Analytics
VITE_ENABLE_ANALYTICS=false

# Note: For demo/development without Supabase, simply omit these variables
# The app will run in DEMO MODE with error messages instead of data
